{
  "name": "activecollab/narrative",
  "description": "Use stories to test and document your API",
  "license": "MIT",
  "require": {
    "php": ">=7.0.0",
    "symfony/console": "~2.3",
    "herrera-io/phar-update": "~1.0",
    "phine/phar": "~1.0",
    "peekmo/jsonpath": "dev-master",
    "michelf/php-markdown": ">1.4",
    "razielanarki/hyperlight": "~0.1",
    "league/csv": "~6.0",
    "smarty/smarty": "~3.1",
    "jbroadway/urlify": "1.0.*@dev",
    "guzzlehttp/guzzle": "~6.0",
    "activecollab/activecollab-feather-sdk": "~2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0"
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "ActiveCollab\\Narrative\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ActiveCollab\\Narrative\\Test\\": "test/src"
    }
  }
}
